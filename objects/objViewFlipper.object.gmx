    // pulled from pxupscale draw code
    var baseWidth = windowWidth;
    var baseHeight = windowHeight;
    var baseX = 0;
    var baseY = 0;

    var aspectRatio = windowWidth / windowHeight;
    var aspectRatioRatio = aspectRatio / (800/608);
    // taller
    if (aspectRatioRatio &lt; 1) {
        baseHeight = windowWidth*608/800;
        baseY = (windowHeight - baseHeight) / 2;
    }
    // wider
    else if (aspectRatioRatio &gt; 1) {
        baseWidth = windowHeight*800/608;
        baseX = (windowWidth - baseWidth) / 2;
